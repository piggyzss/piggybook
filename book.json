{
    "title": "前端面试指北",
    "author": "珊珊",
    "links" : {
        "sidebar" : {
            "前端面试指北" : "https://piggyzss.github.io/piggybook/"
        }
    },
    "plugins": [
        "hide-element",
        "back-to-top-button",
        "chapter-fold",
        "code",
        "-lunr",
        "-search",
        "search-pro",
        "pageview-count",
        "-highlight",
        "prism",
        "prism-themes",
        "tbfed-pagefooter",
        "-sharing", 
        "sharing-plus",
        "changyan"
    ],
    "pluginsConfig": {
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "prism": {
            "css": [
              "prism-themes/themes/prism-xonokai.css"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy shanshan 2020",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "sharing": {
            "google": true,
            "twitter": true,
            "qq": true,
            "weibo": true
        },
        "changyan": {
            "appid": "your changyan's appid",
            "conf": "the conf in the code generate by changyan"
        }
    },
    "styles": {
        "website": "assets/styles/chapter1.css",
        "ebook": "assets/styles/ebook.css",
        "pdf": "assets/styles/pdf.css",
        "mobi": "assets/styles/mobi.css",
        "epub": "assets/styles/epub.css"
    }
}
